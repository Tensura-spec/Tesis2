/// @description A boomerang thrown by a Boomerang Bro

//Inherit event from parent
event_inherited();

//No gravity
gravity = 0;

//Handle movement
ready = 0;
ready2 = 0;
ready3 = 0;

//Thrower id
parent = -1;

//Go up
alarm[0] = 32;

//Horizontal scale
xscale = 1;

//Set the vertical speed
vspeed = -0.75;

//Horizontal speed (DO NOT REMOVE)
xspeed = 0;